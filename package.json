{
  "name": "cdktf_demo",
  "version": "1.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "dev": "rimraf dist && npm run copy-files && env-cmd tsc-watch --project . --outDir ./dist --onSuccess \"nodemon --watch 'dist' ./dist/index.js\"",
    "get": "cdktf get",
    "copy-files": "copyfiles -u 1 src/**/*.template src/**/*.sh dist/",
    "build": "rimraf dist && tsc && npm run copy-files",
    "synth": "env-cmd cdktf synth",
    "plan": "env-cmd cdktf plan",
    "apply": "env-cmd cdktf apply --auto-approve",
    "destroy": "env-cmd cdktf destroy --auto-approve",
    "compile": "tsc --pretty",
    "watch": "tsc --watch",
    "nodemon-help": "nodemon --help",
    "help": "tsc -w --help",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "infracost": "cdktf synth && infracost breakdown --path ./cdktf.out"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@cdktf/provider-azurerm": "4.0.2",
    "cdktf": "^0.14.2",
    "constructs": "^10.1.181"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copyfiles": "^2.4.1",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.9.3"
  }
}
